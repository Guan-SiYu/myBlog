<canvas style=" width:750rpx; height:220rpx" canvas-id="navigation">
</canvas>

<view class="Box" bindtouchstart="touchStart" bindtouchmove="touchMove" bindtouchend="touchEnd">
  <form>
    <view class="title">失卡信息</view>
    <view class="input-wrap fl-r">
      <label>学号</label>
      <input id="studentId" type="number" ></input>
    </view>
    <view class="input-wrap fl-r">
      <label>卡号</label>
      <input id="cardNumber" type="number" ></input>
    </view>
    <view class="input-wrap fl-r">
      <label>姓名</label>
      <input id="name" confirm-type="next"></input>
    </view>
    <view class="tip">{{tip}}</view>
    <view class="title">你的联系方式</view>
    <view class="input-wrap fl-r">
      <label>手机号</label>
      <input id="phone" type="number"></input>
      <button>自动填写</button>
    </view>
    <view class="input-wrap fl-r">
      <label>微信号</label>
      <input id="wechat"></input>
    </view>
    <view class="input-wrap fl-r">
      <label>取卡地点</label>
      <input id="address"></input>
    </view>
    <view class="title">留言</view>
    <textarea id="extra"></textarea>
    <view class="tip">{{tip}}</view>
    <button id="sub">提交</button>
  </form>
</view>
<!-- Back Icon -->
<image src="../../images/back.png" bindtap="backHomePage"></image>